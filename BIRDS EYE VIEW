# src/safeway_guardian/elements/wood_remedies.py
"""
ğŸŒ³ WOOD ELEMENT MEDICINAL SYSTEM
Liver support, stress relief, vitality enhancement remedies
"""

class WoodMedicinalSystem:
    """ğŸŒ³ WOOD: Liver support, detoxification, stress relief remedies"""
    
    def __init__(self):
        self.wood_herbs = {
            'milk_thistle': {
                'name': 'Milk Thistle',
                'latin_name': 'Silybum marianum',
                'primary_actions': ['liver_protection', 'detoxification', 'antioxidant'],
                'dosage_forms': ['tincture', 'capsule', 'tea'],
                'contraindications': ['pregnancy', 'allergy_to_ragweed'],
                'element_affinity': 'WOOD'
            },
            'dandelion_root': {
                'name': 'Dandelion Root',
                'latin_name': 'Taraxacum officinale',
                'primary_actions': ['liver_tonic', 'digestive_stimulant', 'diuretic'],
                'dosage_forms': ['tincture', 'tea', 'decoction'],
                'contraindications': ['gallbladder_issues', 'kidney_problems'],
                'element_affinity': 'WOOD'
            },
            'schisandra': {
                'name': 'Schisandra Berry',
                'latin_name': 'Schisandra chinensis',
                'primary_actions': ['adaptogen', 'liver_protection', 'energy_enhancement'],
                'dosage_forms': ['tincture', 'capsule', 'powder'],
                'contraindications': ['epilepsy', 'pregnancy'],
                'element_affinity': 'WOOD'
            }
        }
        
        self.wood_foods = ['leafy_greens', 'sour_foods', 'lemons', 'limes']
        self.wood_lifestyle = ['morning_exercise', 'stress_management', 'goal_setting']
    
    async def generate_remedies(self, imbalance_level: float, patient_profile: Dict) -> List[MedicinalRemedy]:
        """Generate Wood element remedies based on imbalance severity"""
        remedies = []
        
        if imbalance_level > 0.7:  # Severe imbalance
            remedies.extend(await self._generate_strong_wood_remedies(patient_profile))
        elif imbalance_level > 0.4:  # Moderate imbalance
            remedies.extend(await self._generate_moderate_wood_remedies(patient_profile))
        else:  # Mild imbalance
            remedies.extend(await self._generate_mild_wood_remedies(patient_profile))
        
        # Add lifestyle recommendations
        remedies.extend(await self._generate_wood_lifestyle_remedies(patient_profile))
        
        return remedies
    
    async def _generate_strong_wood_remedies(self, patient_profile: Dict) -> List[MedicinalRemedy]:
        """Strong remedies for severe Wood imbalance"""
        return [
            MedicinalRemedy(
                remedy_id="WOOD-STRONG-1",
                name="Milk Thistle Liver Protocol",
                element="WOOD",
                medicine_type=MedicineType.HERBAL,
                dosage="200mg 3x daily",
                frequency="After meals",
                duration_days=30,
                contraindications=["pregnancy", "allergy_to_ragweed"],
                synergies=["dandelion_root", "turmeric"],
                confidence=0.85
            ),
            MedicinalRemedy(
                remedy_id="WOOD-STRONG-2",
                name="Stress Resilience Formula",
                element="WOOD",
                medicine_type=MedicineType.HERBAL,
                dosage="1 dropper 2x daily",
                frequency="Morning and afternoon",
                duration_days=45,
                contraindications=[],
                synergies=["ashwagandha", "rhodiola"],
                confidence=0.78
            )
        ]

